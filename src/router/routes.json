{
  "/": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Homepage is public",
            "rule": "public"
          }
        ],
        "operator": "OR"
      }
    }
  },
  "/login": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Login page is public",
            "rule": "public"
          }
        ],
        "operator": "OR"
      }
    }
